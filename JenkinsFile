pipeline {
    agent any

    stages {
        stage('Checkout') {
            steps {
                git url: 'https://github.com/atulsharmacsk/CucumberReportPortal'
            }
        }
        stage('Build') {
            steps {
//                 sh 'mvn clean test'
                def mvnHome = tool name: 'Maven', type: 'maven'
                bat "${mvnHome}/bin/mvn clean test"
            }
        }
    }
}
